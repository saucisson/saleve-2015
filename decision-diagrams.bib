@techreport{2009-lh-0,
	Author = {Lucio, Levi and Hostettler, Steve},
	Date-Added = {2010-04-21 10:33:27 +0200},
	Date-Modified = {2010-04-21 10:33:27 +0200},
	Institution = {Centre Universitaire D'Informatique, Université de Genève},
	Note = {Available as http://smv.unige.ch/technical-reports/TR205-MSDD.pdf},
	Number = {205},
	Title = {Multi-Set Decision Diagrams},
	Type = {Technical Report},
	Year = {2009},
	Abstract = {Data Decisions Diagrams (DDD) and Set Decisions Diagrams (SDD) are powerfull tools to encode set of sequences of assignements. However, they represent the existence (or absence) of a particular sequence of assignements not its cardinality. Namely, if we consider Decisions Diagrams as functions, their co-domain would be Booleans. We propose to extend the definition of Data Decisions Diagrams and Set Decisions Diagrams to other co-domains such as natural or real numbers, in order to express cardinalities or probabilities. Similar works have been done for Binary Decision Diagrams (MTBDD) and for Multiway Decision Diagrams (MTMDD).}}

